import java.util.*;
class Main 
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a[] = new int[n];
        for(int i=0;i<n;i++)
        {
            a[i] = sc.nextInt();
        }
        int ans= 1;
        int s = sc.nextInt();
        Arrays.sort(a);
        if(a[0]+a[1]<=s)
        {
            ans = a[0]*a[1];
        }
        System.out.print(ans);
        
    }
}
