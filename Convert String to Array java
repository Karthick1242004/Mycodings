import java.util.*;
class Main{
    public static void main(String[] args) throws Exception {
       Scanner s = new Scanner(System.in);
       String st = s.next();
       int n = s.nextInt();
       char a[] = new char[st.length()];
       for(int i=0;i<st.length();i++)
       {
           a[i] = st.charAt(i);
       }
       System.out.print(a[n]);
}
}
